stock PlayerToPoint(Float:radi, playerid, Float:x, Float:y, Float:z)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:OPos[3];
		new Float:TPos[3];
		GetPlayerPos(playerid, OPos[0], OPos[1], OPos[2]);
		TPos[0] = (OPos[0] -x);
		TPos[1] = (OPos[1] -y);
		TPos[2] = (OPos[2] -z);
		if(((TPos[0] < radi) && (TPos[0] > -radi)) && ((TPos[1] < radi) && (TPos[1] > -radi)) && ((TPos[2] < radi) && (TPos[2] > -radi))) return 1;
	}
	return 0;
}
