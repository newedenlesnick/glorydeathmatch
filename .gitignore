TimeConvert(time)
{
    new minutes1;
    new seconds1;
    new string[128];
    if(time > 59){
        minutes1 = floatround(time/60);
        seconds1 = floatround(time - minutes1*60);
        if(seconds1>9)format(string,sizeof(string),"%d:%d",minutes1,seconds1);
        else format(string,sizeof(string),"%d:0%d",minutes1,seconds1);
    }
    else{
        seconds1 = floatround(time);
        if(seconds1>9)format(string,sizeof(string),"0:%d",seconds1);
        else format(string,sizeof(string),"0:0%d",seconds1);
    }
    return string;
}
