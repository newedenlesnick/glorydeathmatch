public OnPlayerEnterRaceCheckpoint(playerid)
{
    if(InLicense[playerid])
	{
	    for(new i; i<sizeof(LicenseCP)+1; i++)
		{
			if(LicenseTaken[playerid] == i)
			{
				DisablePlayerRaceCheckpoint(playerid);
				if(LicenseTaken[playerid] == sizeof(LicenseCP)) return LicensePassed(playerid);
				if(LicenseTaken[playerid] != sizeof(LicenseCP)-1) SetPlayerRaceCheckpoint(playerid, 0,LicenseCP[i][0], LicenseCP[i][1], LicenseCP[i][2], LicenseCP[i+1][0], LicenseCP[i+1][1], LicenseCP[i+1][2], 15);
				else SetPlayerRaceCheckpoint(playerid, 1, LicenseCP[i][0], LicenseCP[i][1], LicenseCP[i][2], 0, 0, 0, 15);
				LicenseTaken[playerid]++;
				break;
			}
		}
	}
    if(InMission[playerid])
	{
	    if(MissionID[playerid] == 4)
	    {
	        for(new i; i<sizeof(TimeMission)+1; i++)
			{
				if(TimeMissionCP[playerid] == i)
				{
					DisablePlayerRaceCheckpoint(playerid);
					if(TimeMissionCP[playerid] == sizeof(TimeMission)) return MissionWinner(playerid);
					if(TimeMissionCP[playerid] != sizeof(TimeMission)-1) SetPlayerRaceCheckpoint(playerid, 0,TimeMission[i][0], TimeMission[i][1], TimeMission[i][2], TimeMission[i+1][0], TimeMission[i+1][1], TimeMission[i+1][2], 15);
					else SetPlayerRaceCheckpoint(playerid, 1, TimeMission[i][0], TimeMission[i][1], TimeMission[i][2], 0, 0, 0, 15);
					TimeMissionCP[playerid]++;
					break;
				}
			}
	    } else {
	        MissionWinner(playerid);
	    }
	}
	if(ActStatus >= 2 && InAct[playerid] && actInfo [ TurnedAct ] [ ActType ] == ACT_RACE )
	{
		switch(actInfo[TurnedAct][ActNum])
		{
		    case 11:
		    {
				for(new i; i<sizeof(FallingCP)+1; i++)
				{
					if(ActCP[playerid] == i)
					{
					    DisablePlayerRaceCheckpoint(playerid);
						if(ActCP[playerid] == 1) return RaceWinner(playerid);
						break;
					}
				}
		    }
		    case 10:
		    {
				for(new i; i<sizeof(ClimbCP)+1; i++)
				{
					if(ActCP[playerid] == i)
					{
					    DisablePlayerRaceCheckpoint(playerid);
						if(ActCP[playerid] == 9) return RaceWinner(playerid);
					    if(ActCP[playerid] != 8) SetPlayerRaceCheckpoint(playerid, 0,ClimbCP[i][0], ClimbCP[i][1], ClimbCP[i][2], ClimbCP[i+1][0], ClimbCP[i+1][1], ClimbCP[i+1][2], 15);
						else SetPlayerRaceCheckpoint(playerid, 1, ClimbCP[i][0], ClimbCP[i][1], ClimbCP[i][2], 0, 0, 0, 15);
						ActCP[playerid]++;
						break;
					}
				}
		    }
		    case 9:
		    {
				for(new i; i<sizeof(ParkourCP)+1; i++)
				{
					if(ActCP[playerid] == i)
					{
					    DisablePlayerRaceCheckpoint(playerid);
						if(ActCP[playerid] == 1) return RaceWinner(playerid);
					    if(ActCP[playerid] == 0) SetPlayerRaceCheckpoint(playerid, 0, ParkourCP[i][0], ParkourCP[i][1], ParkourCP[i][2], ParkourCP[i][0], ParkourCP[i][1], ParkourCP[i][2], 15);
						ActCP[playerid]++;
						break;
					}
				}
		    }
		    case 8:
		    {
