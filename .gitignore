stock GetNum(Num)
{
    new sMoney[13];
    format(sMoney,sizeof(sMoney),"%d",Num);
    if(strlen(sMoney) < 4) return sMoney;
    new iPos = strlen(sMoney), iCount = 1;
    while(iPos > 0)
    {
        if(iCount == 4)
        {
            iCount = 0;
            strins(sMoney,",",iPos,1);
            iPos ++;
        }
        iCount ++;
        iPos --;
    }
    return sMoney;
}
