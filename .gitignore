function EndCall(id1,id2,reason)
{
	if(reason == 1) // no answer.
	{
		if(InCall[id1] == 1)
		{
			InCall[id1] = 0;
			SetPlayerSpecialAction(id1,SPECIAL_ACTION_STOPUSECELLPHONE);
			format(str,sizeof(str),".לא ענה לשיחתך \"%s\"",GetName[id2]);
			SendClientMessage(id1,0x74BDF9FF,str);
		}
		format(str,sizeof(str),".\"%s\" לא ענית לשיחה",GetName[ id1 ]);
		SendClientMessage(id2,0x74BDF9FF,str);
        InCall[id1] = 0;
        InCall[id2] = 0;
		InCall[id1] = 0;
		InCall[id2] = 0;
		Tallking[id1] = 0;
		Tallking[id2] = 0;
		TallkingID[id1] = -1;
		TallkingID[id2] = -1;
		IsCalling[id1] = 0;
	}
	else if(reason == 2) // no money.
	{
		SendClientMessage(id1, 0x74BDF9FF, ".השיחה נותקה, אין לך מספיק כסף");
		SendClientMessage(id2, 0x74BDF9FF, ".השיחה נותקה מאחר לבעל השיחה אין מספיק כסף");
        SetPlayerSpecialAction(id1,SPECIAL_ACTION_STOPUSECELLPHONE);
		SetPlayerSpecialAction(id2,SPECIAL_ACTION_STOPUSECELLPHONE);
		InCall[id1] = 0;
        InCall[id2] = 0;
		InCall[id1] = 0;
		InCall[id2] = 0;
		Tallking[id1] = 0;
		Tallking[id2] = 0;
		TallkingID[id1] = -1;
		TallkingID[id2] = -1;
		IsCalling[id1] = 0;
	}
	else if(reason == 3) // player disconnected / hangup.
	{
		InCall[id1] = 0;
		InCall[id2] = 0;
		Tallking[id1] = 0;
		Tallking[id2] = 0;
		TallkingID[id1] = -1;
		TallkingID[id2] = -1;
		IsCalling[id1] = 0;
	}
	return 1;
}
function CheckCall()
{
	StartLoop(i)
	{
		if(InCall[i] == 1 && Tallking[i] == 1 && IsDead[i] == 0 && IsCalling[i] == 1)
		{
			if(sGetPlayerMoney(i) >= 1) sGivePlayerMoney(i, -1);
			if(sGetPlayerMoney(i) < 1) EndCall(i,TallkingID[i],2);
		}
	}
	return 1;
}

