function StartCD(count)
{
    new cdt[32];
	format(cdt, sizeof(cdt), "~w~%d", count);
	if(count == 2) format(cdt, sizeof(cdt), "~r~Ready", count);
	else if(count == 1) format(cdt, sizeof(cdt), "~y~Set", count);
	StartLoop(i) if(IsPlayerConnected(i) && InAct[i]) GameTextForPlayer(i, cdt, 1000, 6);
	if(count > 0)
	{
		count--;
		ActStartCD = SetTimerEx("StartCD", 1000, false, "d", count);
	}else{
		StartLoop(i) if(IsPlayerConnected(i) && InAct[i])
		{
			GameTextForPlayer(i, "~g~Go Go Go", 1000, 6);
			TogglePlayerControllable(i, 1);
		}
		ActStatus = 3;
	    SendFormatMessageToAll( 0xFFFFFFFF, "~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ [ {A3D1FF}%s{FFFFFF} ] ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~", actInfo[TurnedAct][ActName]);
    	SendFormatMessageToAll(0xA3D1FFFF, ".החלה, בהצלחה לכל המשתתפים {FFFFFF}%s {A3D1FF}הפעילות", actInfo[TurnedAct][ActName]);
    	SendFormatMessageToAll(0xA3D1FFFF, ".{FFFFFF}%d/%d {A3D1FF}:כמות המשתתפים בפעילות", ActPlayers, actInfo[TurnedAct][ActMaxPlayers]);
    	SendFormatMessageToAll(0xA3D1FFFF, ".הפרס למנצח הוא {FFFFFF}%d + %d$ {A3D1FF}נקודות", actInfo[TurnedAct][ActPoint], actInfo[TurnedAct][ActPrize]);
	    SendFormatMessageToAll( 0xFFFFFFFF, "~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ [ {A3D1FF}%s{FFFFFF} ] ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~", actInfo[TurnedAct][ActName]);
    	aMoney = actInfo[TurnedAct][ActPrize];
    	aPoint = actInfo[TurnedAct][ActPoint];
    	KillTimer(ActStartCD);
	}
	return 1;
}
stock RaceWinner( playerid )
{
    if ( InAct [ playerid ] && ActStatus >= 2 )
	{
	    SendFormatMessageToAll( 0xFFFFFFFF, "~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ [ {A3D1FF}%s{FFFFFF} ] ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~", actInfo[TurnedAct][ActName]);
		SendFormatMessageToAll(0xA3D1FFFF, ".הסתיימה {FFFFFF}%s {A3D1FF}הפעילות", actInfo[TurnedAct][ActName]);
		SendFormatMessageToAll(0xA3D1FFFF, ".{FFFFFF}%s {A3D1FF}:הזוכה בפעילות הוא", GetName[playerid]);
		SendFormatMessageToAll(0xA3D1FFFF, ".הפרס בו הוא זכה {FFFFFF}%d + %d$ {A3D1FF}נקודות", actInfo[TurnedAct][ActPoint], actInfo[TurnedAct][ActPrize]);
	    SendFormatMessageToAll( 0xFFFFFFFF, "~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ [ {A3D1FF}%s{FFFFFF} ] ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~", actInfo[TurnedAct][ActName]);
        sGivePlayerMoney(playerid, aMoney);
        pFile[playerid][pPoints] = pFile[playerid][pPoints] + aPoint;
        pFile[playerid][pWonAct] = pFile[playerid][pWonAct] + 1;
        format(MString, 256, "Points: ~w~%d", pFile[playerid][pPoints]);
		TextDrawSetString(Points[playerid], MString);
        ResetPlayerWeapons(playerid);
        SetPlayerWeather(playerid,2);
        SpawnPlayer(playerid);
        SetPlayerHealth(playerid, 99);
		SetPlayerArmour(playerid, 0);
        InAct[playerid] = 0;
        SetPlayerVirtualWorld(playerid,0);
        SetPlayerInterior(playerid,0);
        TurnedAct = 0;
        ActPlayers = 0;
        ActStatus = 0;
        KillTimer(RaceCheck);
		for(new b; b<sizeof(PlayerVeh); b++) if(InAct[b]) DestroyVehicle(PlayerVeh[b]);
		StartLoop(i)
		{
		    ActCP[i] = 0;
			if(InAct[i])
			{
				OnPlayerSpawn(i);
				ActCP [i] = 0;
				InAct[i] = false;
				DisablePlayerRaceCheckpoint(i);
			}
		}
	}
 	return 1;
}
function Winner(playerid)
{
    if(InAct[playerid] && ActStatus >= 2)
	{
	    ActPlayers--;
		InAct[playerid] = 0;
		if(actInfo[TurnedAct][ActType] == ACT_RACE) DisablePlayerRaceCheckpoint(playerid);
		if(actInfo[TurnedAct][ActType] == ACT_CARWAR) Delete3DTextLabel(ActCars[PlayerVeh[playerid]]),DestroyVehicle(PlayerVeh[playerid]);
		SendClientMessage(playerid, 0xFF0000FF, ".נפסלת בפעילות, נסה פעם הבאה");
		RemovePlayerAttachedObject(playerid, 0);
		CanBasket[playerid] = false;
		CanShoot[playerid] = false;
		ResetPlayerWeapons(playerid);
