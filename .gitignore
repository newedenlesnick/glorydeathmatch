public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(newstate == PLAYER_STATE_DRIVER)
	{
        if(fexist(VFile(GetPlayerVehicleID(playerid))))
        {
			if(pFile[playerid][pLicense] == 0)
			{
			    ClearAnimations(playerid);
			    SendClientMessage(playerid,0xFF0000FF,"/License » אין ברשותך רשיון ולכן לא תוכל לנסוע ברכב קנוי");
			}
		}
	}
	if(oldstate == PLAYER_STATE_DRIVER) vJump{playerid} = 0 , NSh{playerid} = 0 , pJump{playerid} = 0;
	if(oldstate == PLAYER_STATE_DRIVER && newstate == PLAYER_STATE_ONFOOT && InMission[playerid]) \
	MissionFailed(playerid);
	if(oldstate == PLAYER_STATE_DRIVER && ActStatus == 3 && actInfo[TurnedAct][ActType] == ACT_CARWAR||oldstate == PLAYER_STATE_DRIVER && ActStatus == 3 && actInfo[TurnedAct][ActType] == ACT_RACE)
	{
		if(actInfo[TurnedAct][ActType] != ACT_WAR) Winner(playerid);
	}
	return 1;
}
