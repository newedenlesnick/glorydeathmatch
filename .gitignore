		mcd --;
		MissionCD[MissionID[playerid]] = SetTimerEx("MissionCDT", 1000, false, "id", playerid, mcd);
	}
	else
	{
	    KillTimer(MissionCD[MissionID[playerid]]);
	    GameTextForPlayer(playerid, "~r~Mission Failed", 2000, 4);
	    MissionFailed(playerid);
	}
	return 1;
}
